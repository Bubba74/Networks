
public class Perspective {

	/*
		Holds x and y coords
	private double camera_x, camera_y;

	public Perspective (double x, double y){
		camera_x = x;
		camera_y = y;
	}

	public void setX(double x){
		camera_x = x;
	}
	public double getX(){
		return camera_x;
	}
	public void setY (double y){
		camera_y = y;
	}
	public double getY (){
		return camera_y;
	}

	public void setLoc (double x, double y){
		camera_x = x;
		camera_y = y;
	}
}//Perspective class
